{
  "name": "generate-external-type",
  "description": "Generate external type from files",
  "homepage": "https://github.com/nayounsang/generate-external-type",
  "keywords": [
    "type",
    "external",
    "generate",
    "typescript",
    "scripts",
    "file"
  ],
  "version": "1.0.2",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "lint": "eslint --fix",
    "release": "changeset publish",
    "changeset": "changeset",
    "version": "changeset version",
    "prepublishOnly": "npm run build"
  },
  "author": {
    "name": "Younsang Na",
    "email": "nayounsang72@gmail.com",
    "url": "https://github.com/nayounsang"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@eslint/js": "^9.34.0",
    "@rollup/plugin-commonjs": "^27.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^12.0.0",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "eslint": "^9.34.0",
    "rollup": "^4.12.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "glob": "^11.0.3"
  }
}
